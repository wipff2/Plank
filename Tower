local player = game:GetService("Players").LocalPlayer
local character = player.Character
local hammer = character:FindFirstChild("Hammer") or player.Backpack:FindFirstChild("Hammer")
local tower = workspace.Buildings:FindFirstChild("Tower")

if not tower or not hammer then
    warn("Tower or Hammer not found!")
    return
end

local buildPlank = hammer:FindFirstChild("BuildPlank")
if not buildPlank then
    warn("BuildPlank event not found!")
    return
end

